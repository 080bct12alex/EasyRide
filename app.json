{
  "expo": {
    "name": "EasyRyde",
    "slug": "easyryde",
    "version": "1.0.0",
    "orientation": "portrait",
    "runtimeVersion": "1.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "EasyRyde",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F80ED"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alexstha03.easyryde",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://uber.com/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://uber.com/"
      },
      "eas": {
        "projectId": "f98f07e0-8bb1-45aa-ab26-0651feb7d31d"
      }
    }
  }
}
